<nav class="navbar navbar-light fixed-top flex-md-nowrap p-0" style="background-color: #ffff00;">
      <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="#">SparkJoy</a>
      <a><h2>管理者</h2></a>
      <ul class="navbar-nav px-3">
        <li class="nav-item text-nowrap">
          <a class="nav-link" href="#">サインアウト</a>
        </li>
      </ul>
    </nav>
<div class="admin-page">
<div class="container-fluid">
  <div class="row">
    <nav class="col-md-2 d-none d-md-block bg-light sidebar">
          <div class="sidebar-sticky">
            <ul class="nav flex-column">
              <li class="nav-item">
                  <span data-feather="home">
                  ダッシュボード</span>
              </li>
              <li class="nav-item">
                  ユーザー一覧
              </li>
              <li class="nav-item">
                  アイテムカテゴリー一覧
              </li>
            </ul>
    </nav>
<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
<h1>ユーザー一覧ページ</h1>
<p>Find me in app/views/admins/admins/index.html.erb</p>
<table class="table table-striped table-sm user-table">
      <thead>
        <tr>
          <th>会員番号</th>
          <th>ユーザー名</th>
          <th>年齢</th>
          <th>性別</th>
          <th>登録アイテム数</th>
          <th>登録日</th>
          <th></th>
        </tr>
      </thead>
       <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.name %></td>
            <td><%= user.age %></td>
            <td><%= user.gender %></td>
            <td><%= user.items.count %></td>
            <td><%= user.created_at %></td>
            <td>
              <%= link_to '詳細', admins_user_path(user),
               class:"btn btn-outline-secondary"%>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </main>
  </div>
</div>
</div>

