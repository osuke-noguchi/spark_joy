<nav class="navbar navbar-light fixed-top flex-md-nowrap p-0" style="background-color: #ffff00;">
      <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="#">SparkJoy</a>
      <a><h2>管理者</h2></a>
      <ul class="navbar-nav px-3">
        <li class="nav-item text-nowrap">
          <a class="nav-link" href="#">サインアウト</a>
        </li>
      </ul>
    </nav>
<div class="admin-page">
<div class="container-fluid">
  <div class="row">
<%= render "admins/shared/sidebar" %>
<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-4">
<h1>ユーザー一覧ページ</h1><h2>アイテム：<%= Item.count %>名</h2>
<table class="table table-striped table-sm user-table">
      <thead>
        <tr>
          <th>カテゴリー</th>
          <th>ブランド名</th>
          <th>金額</th>
          <th></th>
        </tr>
      </thead>
       <tbody>
        <% @items.each do |item| %>
          <tr>
            <td><%= item.category.category_name %></td>
            <td><%= item.brand %></td>
            <td><%= item.price %></td>
            <td>
              <%= link_to '詳細', admins_user_path(user),
               class:"btn btn-outline-secondary"%>
               <%= link_to '削除', admins_user_path(user), method: :delete,
           data: { confirm: '本当に削除しますか?' }, class: "btn btn-outline-danger" %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
</main>
</div>
</div>
</div>
