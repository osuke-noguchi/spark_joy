<nav class="navbar navbar-dark fixed-top bg-dark flex-md-nowrap p-0 shadow">
      <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="#">会社名</a>
    </nav>

<div class="container">
  <div class="row">
    <div class="col-md-12">
    <div class="admin-page">
      <h1>ユーザー一覧ページ</h1>
      <p>Find me in app/views/admins/admins/index.html.erb</p>
　　　<table class="table table-striped table-sm user-table">
      <thead>
        <tr>
          <th>会員番号</th>
          <th>ユーザー名</th>
          <th>年齢</th>
          <th>性別</th>
          <th>登録アイテム数</th>
          <th>登録日</th>
          <th></th>
        </tr>
      </thead>
       <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.name %></td>
            <td><%= user.age %></td>
            <td><%= user.gender %></td>
            <td><%= user.items.count %></td>
            <td><%= user.created_at %></td>
            <td>
              <%= link_to '詳細', admins_user_path(user),
               class:"btn btn-outline-secondary"%>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    </div>
    </div>
    </div>
</div>

